        {
            "name": "Luiz Pires",
            "email": "luiz@kw13.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/kw13code/TDD-CLEAN-ARCHITECTURE-PHP/issues",
        "source": "https://github.com/kw13code/TDD-CLEAN-ARCHITECTURE-PHP",
        "rss": "https://github.com/kw13code/TDD-CLEAN-ARCHITECTURE-PHP/releases.atom"
    },
    "keywords": [
        "skeleton",
        "clean architecture",
        "TDD"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {},
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test:staged": "./vendor/bin/phpunit && php vendor/bin/phpstan analyse src tests --level 7",
        "test:ci": "./vendor/bin/phpunit --coverage-html tests/coverage-report "
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^8.0",
        "phpstan/phpstan": "^0.12.14",
        "phpstan/phpstan-phpunit": "^0.12.6"
    },
    "autoload": {
        "psr-4": {
            "KW13\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}